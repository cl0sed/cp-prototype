# PostgreSQL Settings (used by db service in docker-compose.yml)
POSTGRES_USER=user_placeholder
POSTGRES_PASSWORD=password_placeholder
POSTGRES_DB=db_name_placeholder

# SuperTokens Settings (Managed Service)
SUPERTOKENS_CONNECTION_URI=https://try.supertokens.com # Replace with your actual core URI if not using the free tier
SUPERTOKENS_API_KEY= # Replace with your actual API key

# LLM Configuration (Haystack)
LLM_API_KEY= # Replace with your LLM API key
LLM_MODEL=google/gemini-2.5-flash-preview # Default LLM model (matches config.py)
LLM_API_URL= # Optional custom API URL

# --- Prompt and Tool Versioning Configuration ---
DEFAULT_CHAT_PIPELINE_TAG="dev" # Required: Default tag for the chat pipeline (matches config.py default)
# PIPELINE_TAGS_CONFIG="./pipeline-tags.yaml" # Optional: Path to the pipeline tags override YAML file (matches config.py default)
# DEFAULT_PROMPT_VERSION="v1" # Optional: Default prompt version if no override is found (matches config.py default)

